{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "routes/**",
            "middlewares/**",
            "controllers/**",
            "models/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "PORT": "5000"
    }
  }